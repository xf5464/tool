<view class="main-view">

  <view class="row-view" style="height:17vh">
    <text class="left-text">qq群</text>
    <text class="right-text" style="color:#999">{{qqGroup}}</text>
    <button data-copy-value="{{qqGroup}}" class="btn1" hover-class="btnClick" type="primary" bindtap="copyText">复制</button>
  </view>

  <view class="row-view" style="height:17vh">
    <text class="left-text">网易云音乐电台</text>

    <text class="right-text" style="color:#999">{{music163}}</text>
    <button data-copy-value="{{music163}}" class="btn1" hover-class="btnClick" type="primary" bindtap="copyText">复制</button>
  </view>

  <view class="row-view" style="height:17vh">
    <text class="left-text">斗鱼</text>
    <text class="right-text" style="color:#999">{{douyu}}</text>
    <button data-copy-value="{{douyu}}" class="btn1" hover-class="btnClick" type="primary" bindtap="copyText">复制</button>
  </view>

  <view class="row-view" style="height:17vh">
    <text class="left-text">微博</text>
    <text class="right-text" style="color:#999">{{weibo}}</text>
    <button data-copy-value="{{weibo}}" class="btn1" hover-class="btnClick" type="primary" bindtap="copyText">复制</button>
  </view>

</view>